{
  "name": "Pebx film reader",
  "description": "Make the current page red",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "pebx.pl film api"
  },
  "manifest_version": 2,
  "permissions": [ "webRequest","*://*.pebx.pl/*","*://*.netflix.com/*", "*://*.imdb.com/*", "*://*.unogs.com/*", "*://*.themoviedb.org/*"],
  "content_scripts": [
    {
    "matches": [
      "https://pebx.pl/forum/*"
    ],
    "js": ["check.js"]
   }
  ]
}
