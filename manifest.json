{
  "name": "Pebx film reader",
  "description": "Make the current page red",
  "version": "2.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "pebx.pl film api"
  },
  "manifest_version": 2,
  "permissions": [ "storage", "*://*.pebx.pl/*", "https://www.omdbapi.com/*" ,"webRequest", "webRequestBlocking" ],
  "content_scripts": [
    {
      "matches": [
        "https://pebx.pl/forum/*"
      ],
      "js": ["check.js"]
    }
  ]
}
